{
    "schemas": {
        "Todo": {
            "type": "object",
            "properties": {
                "id": {
                    "type": "string",
                    "example": "123"
                },
                "description": {
                    "type": "string",
                    "example": "make world awsome"
                },
                "isCompleted": {
                    "type": "boolean",
                    "example": false
                }
            }
        }
    },
    "responses": {
        "GetAllTodoResponse": {
            "description": "List of todos",
            "content": {
                "application/json": {
                    "schema": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/Todo"
                        }
                    }
                }
            }
        }
    }
}
